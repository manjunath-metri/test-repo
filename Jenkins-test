pipeline{
    agent any
    stages{
        stage("GIT checkout"){
            steps{
                git branch: 'main', url: 'https://github.com/manjunath-metri/test-repo.git'
                
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
                sh "mv target/*.war target/myweb.war"
            }
         }
    }
}
